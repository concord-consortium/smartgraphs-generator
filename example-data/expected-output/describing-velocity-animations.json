          ],
          "legends": {
            "title": "legend",
            "type": "name",
            "referenceDatadef": "",
            "datadefs": []
          },
          "activeDatadefs": [
            "position-data"
          ]
        }
      },
      "tools": [
        {
          "name": "animation",
          "setup": {
            "pane": "top",
            "hideGraph": true,
            "duration": 9000,
            "channelWidth": 70,
            "staticImages": [
              {
                "name": "finish",
                "image": "finish.png",
